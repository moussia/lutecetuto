<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plug-in>
    <name>testmoussia</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0-SNAPSHOT</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>testmoussia.plugin.description</description>
    <provider>testmoussia.plugin.provider</provider>
    <provider-url>http://moussia</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>2020</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>TESTMOUSSIA_MANAGEMENT</feature-id>
            <feature-title>testmoussia.adminFeature.ManageUsers.name</feature-title>
            <feature-description>testmoussia.adminFeature.ManageUsers.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/testmoussia/ManageUsers.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
        </admin-feature>
    </admin-features>

    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>user</application-id>
            <application-class>fr.paris.lutece.plugins.testmoussia.web.UserXPage</application-class>
        </application>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
    </portlets>
</plug-in>